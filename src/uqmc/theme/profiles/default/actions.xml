<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <object name="user" meta_type="CMF Action Category">
    <object name="facebook-after" meta_type="CMF Action" i18n:domain="plone" insert-after="*">
      <property name="title" i18n:translate="">Facebook</property>
      <property name="description" i18n:translate="">UQMC Facebook Group</property>
      <property name="url_expr">string:https://www.facebook.com/groups/uqmountainclub/</property>
      <property name="icon_expr"></property>
      <property name="available_expr"></property>
      <property name="permissions">
        <element value="View"/>
      </property>
      <property name="visible">True</property>
    </object>

    <object name="forum-after" meta_type="CMF Action" i18n:domain="plone" insert-after="*">
      <property name="title" i18n:translate="">Forum</property>
      <property name="description" i18n:translate="">UQMC Forum</property>
      <property name="url_expr">string:http://uqmc.yuku.com/</property>
      <property name="icon_expr"></property>
      <property name="available_expr"></property>
      <property name="permissions">
        <element value="View"/>
      </property>
      <property name="visible">True</property>
    </object>
    <object name="login">
      <property name="title" i18n:translate="">Member Entrance</property>
    </object>
  </object>

  <object name="portal_tabs" meta_type="CMF Action Category">
    <object name="index_html" meta_type="CMF Action" i18n:domain="plone" remove="True" />

    <object name="about" meta_type="CMF Action" i18n:domain="plone" insert-before="logout">
     <property name="title" i18n:translate="">About</property>
     <property name="description" i18n:translate="">About UQ Mountain Club</property>
     <property name="url_expr">string:${globals_view/navigationRootUrl}/about</property>
     <property name="icon_expr"></property>
     <property name="available_expr"></property>
     <property name="permissions"></property>
     <property name="visible">True</property>
    </object>

    <object name="membership" meta_type="CMF Action" i18n:domain="plone" insert-before="logout">
     <property name="title" i18n:translate="">Membership</property>
     <property name="description" i18n:translate="">UQ Mountain Club Membership</property>
     <property name="url_expr">string:${globals_view/navigationRootUrl}/membership</property>
     <property name="icon_expr"></property>
     <property name="available_expr"></property>
     <property name="permissions"></property>
     <property name="visible">True</property>
    </object>

    <object name="trips-events" meta_type="CMF Action" i18n:domain="plone" insert-before="logout">
     <property name="title" i18n:translate="">Trips&amp;events</property>
     <property name="description" i18n:translate="">Trips &amp; events coming up</property>
     <property name="url_expr">string:${globals_view/navigationRootUrl}/trips-events</property>
     <property name="icon_expr"></property>
     <property name="available_expr"></property>
     <property name="permissions"></property>
     <property name="visible">True</property>
    </object>

    <object name="useful-info" meta_type="CMF Action" i18n:domain="plone" insert-before="logout">
     <property name="title" i18n:translate="">Useful info</property>
     <property name="description" i18n:translate="">Useful information</property>
     <property name="url_expr">string:${globals_view/navigationRootUrl}/useful-info</property>
     <property name="icon_expr"></property>
     <property name="available_expr"></property>
     <property name="permissions"></property>
     <property name="visible">True</property>
    </object>

    <object name="training" meta_type="CMF Action" i18n:domain="plone" insert-before="logout">
     <property name="title" i18n:translate="">Training</property>
     <property name="description" i18n:translate="">Training</property>
     <property name="url_expr">string:${globals_view/navigationRootUrl}/training</property>
     <property name="icon_expr"></property>
     <property name="available_expr"></property>
     <property name="permissions"></property>
     <property name="visible">True</property>
    </object>

    <object name="executive" meta_type="CMF Action" i18n:domain="plone" insert-before="logout">
     <property name="title" i18n:translate="">Execs</property>
     <property name="description" i18n:translate="">Executives of UQ Mountain Club</property>
     <property name="url_expr">string:${globals_view/navigationRootUrl}/executives</property>
     <property name="icon_expr"></property>
     <property name="available_expr"></property>
     <property name="permissions"></property>
     <property name="visible">True</property>
    </object>

    <object name="contacts" meta_type="CMF Action" i18n:domain="plone" insert-before="logout">
     <property name="title" i18n:translate="">Contacts</property>
     <property name="description" i18n:translate="">Contacting UQ Mountain Club</property>
     <property name="url_expr">string:${globals_view/navigationRootUrl}/contacts</property>
     <property name="icon_expr"></property>
     <property name="available_expr"></property>
     <property name="permissions"></property>
     <property name="visible">True</property>
    </object>

  </object>

</object>
